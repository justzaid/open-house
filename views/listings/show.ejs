<%- include('../partials/head') %>
    <div class="container">
        <div class="card mt-2" style="width: 30vw;">
            <img src="https://images.pexels.com/photos/106399/pexels-photo-106399.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=1" class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title">$<%= listing.price %></h5>
                    <p class="card-text"><%= listing.streetAddress %></p>
                    <p class="card-text"><%= listing.size %> m/sq</p>
                    <p class="text-muted">Listed by: <%= listing.owner.username || 'unknown' %></p>
                    <a href="/listings" class="btn btn-primary">Go back</a>
                </div>
        </div>
    </div>


<%- include('../partials/footer') %>